
<p id="notice"><%= notice %></p>
<div class="room_management_show">
  <div class="ui grid">
    <div class="four wide column" style="padding-top: 5em">
      <%= render partial: "room_management_sidebar", locals: {active: 'facility'} %>
    </div>
    <div class="twelve wide column">
      <p>
        <strong>Room:</strong>
        <%= @room.room_name %>
      </p>
      <div id="room_facility">
        <%= render "room_facility_list" %>
      </div>
    </div>
  </div>
</div>
<script>
  $("#manager_room_management_path").addClass("active");
</script>
