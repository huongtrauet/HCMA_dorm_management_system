<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl" crossorigin="anonymous">
<div class="homepage_management_show" style="width: 70%; margin: 10rem auto">
  <div class="title">
    <h1><%= @post.title %></h1>
  </div>
  <div class="time" style="margin: 1.5rem 0">
    <span><%= @post.created_at.to_date %></span>
  </div>
  <div class="image">
    <img src="/assets/ava.jpg" style="width: 100%">
  </div>
  <div class="owl-carousel">
    <% for i in 0..5 %>
      <div> 
         <img src="/assets/chic.jpg" style="width: 100%">
      </div>
    <% end %>
  </div>
  <div class="content" style="margin-top: 2rem" >
    <p style="font-size: 1.1rem;line-height: 2rem;">
    <%= @post.content %>
    </p>
  </div>
  <div class="footer">
  </div>
</div>
<%= content_for(:after_js) do %>
  <script>
  $(document).ready(function(){
    $('.owl-carousel').owlCarousel();
  });
</script>
<% end %>
