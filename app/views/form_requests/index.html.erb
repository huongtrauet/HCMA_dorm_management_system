<p id="notice"><%= notice %></p>

<h1>Form Requests</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @form_requests.each do |form_request| %>
      <tr>
        <td><%= form_request.type %></td>
        <td><%= link_to 'Show', form_request %></td>
        <td><%= link_to 'Edit', edit_form_request_path(form_request) %></td>
        <td><%= link_to 'Destroy', form_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Form Request', new_form_request_path %>
