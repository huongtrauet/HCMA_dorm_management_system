<p id="notice"><%= notice %></p>

<h1>Facility Reports</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @facility_reports.each do |facility_report| %>
      <tr>
        <td><%= facility_report.title %></td>
        <td><%= link_to 'Show', facility_report %></td>
        <td><%= link_to 'Edit', edit_facility_report_path(facility_report) %></td>
        <td><%= link_to 'Destroy', facility_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Facility Report', new_facility_report_path %>
